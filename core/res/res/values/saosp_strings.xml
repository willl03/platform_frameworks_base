<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Restart Dialog -->
    <string name="global_action_reboot">Restart</string>
    <!-- label for item that screenshots in phone options dialog -->
    <string name="global_action_screenshot">Screenshot</string>
    <string name="global_action_search">Search</string>
    <string name="global_action_voicesearch">Voice Search</string>
    <string name="global_action_torch">Flashlight</string>
    <string name="global_action_current_user">Current</string>
    <string name="global_action_screen_record">Screen record</string>
    <!-- Button to restart the phone in restart menu -->
    <string name="reboot_reboot">Restart</string>
    <!-- Button to restart the phone into recovery in restart menu-->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to restart the phone into bootloader in restart menu -->
    <string name="reboot_bootloader">Bootloader</string>
    <!-- Button to soft restart the device in restart menu -->
    <string name="reboot_soft">Soft restart</string>
    <!-- Restart Confirmation Dialog. -->
    <string name="reboot_confirm" product="tablet">Your tablet will restart.</string>
    <string name="reboot_confirm" product="default">Your phone will restart.</string>
    <!-- Restart Progress Dialog. This is shown if the user chooses to restart the phone. -->
    <string name="reboot_progress">Restarting\u2026</string>

    <!-- kill app long press back -->
    <string name="app_killed_message"><xliff:g id="string">%s</xliff:g> killed</string>
    <string name="no_app_killed_message">Nothing to kill</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_modifyOverlays">modify theme overlays</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_modifyOverlays">Allows the app to make modifications to theme overlays using OverlayManagerService</string>

    <!-- Vendor mismatched fingerprint warning -->
    <string name="system_error_vendorprint">Your vendor image does not match the system. Please flash the <xliff:g id="string">%s</xliff:g> vendor image for your device</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_restartUI">restart the system bars</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_restartUI">Restart SystemUIService so that system bars can load themed resources</string>

    <!-- Single handed mode -->
    <string name="singlehandmode_click_hint_message">Touch outside the screen to exit mini screen view</string>

    <!-- SOFTAP framework  -->
    <string name="tethered_notification_no_device_message">No connected device</string>
    <string name="tethered_notification_one_device_message"><xliff:g id="count">%1$s</xliff:g> connected device</string>
    <string name="tethered_notification_multi_device_message"><xliff:g id="count">%1$s</xliff:g> connected devices</string>

    <!-- Assist manager -->
    <string name="no_assist_found">No assist manager found</string>

